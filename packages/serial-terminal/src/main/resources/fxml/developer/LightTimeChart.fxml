<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="transmitter.ui.developer.LightTimeChart">
   <children>
      <ScrollPane fx:id="chartScroll" fitToHeight="true" fitToWidth="true" VBox.vgrow="ALWAYS">
         <content>
            <StackPane fx:id="chartStack" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
         </content>
      </ScrollPane>
      <HBox alignment="CENTER_LEFT" prefHeight="50.0" spacing="10.0">
         <children>
            <Label text="Byte duration (ms)" />
            <Spinner fx:id="durationSpinner" editable="true" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
            <JFXButton fx:id="prevButton" layoutX="20.0" layoutY="20.0" text="Prev &lt;" />
            <JFXButton fx:id="nextButton" text="Next &gt;" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@../../stylesheet/theme.css" />
      <URL value="@../../stylesheet/template.css" />
   </stylesheets>
</VBox>